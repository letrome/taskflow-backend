meta {
  name: Delete project tag
  type: http
  seq: 6
}

delete {
  url: {{baseurl}}/tags/{{tag_id}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 400
  res.body.status: eq error
  res.body.message: eq Tag is used by a task
}

script:post-response {
  bru.setVar("tag_id_1", res.body.id);
}

settings {
  encodeUrl: true
  timeout: 0
}
