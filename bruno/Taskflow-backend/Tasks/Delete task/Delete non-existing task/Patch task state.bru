meta {
  name: Patch task state
  type: http
  seq: 2
}

delete {
  url: {{baseurl}}/tasks/{{invalid_task_id}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 404
  res.body.status: eq error
  res.body.message: eq Task not found
}

script:pre-request {
  bru.setVar("invalid_task_id", "6961082914d673fca73a59e2");
}

settings {
  encodeUrl: true
  timeout: 0
}
