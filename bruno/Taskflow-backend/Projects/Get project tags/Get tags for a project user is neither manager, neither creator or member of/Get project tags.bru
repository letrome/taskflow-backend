meta {
  name: Get project tags
  type: http
  seq: 4
}

get {
  url: {{baseurl}}/projects/{{invalid_project_id}}/tags
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 404
  res.body.status: eq error
  res.body.message: eq Project not found
}

script:pre-request {
  bru.setVar("invalid_project_id", "invalid")
}

settings {
  encodeUrl: true
  timeout: 0
}
