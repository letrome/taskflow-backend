meta {
  name: Get tasks (filter by title)
  type: http
  seq: 2
}

get {
  url: {{baseurl}}/projects/{{project_id}}/tasks?search=AWESOME
  body: none
  auth: bearer
}

params:query {
  search: AWESOME
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
}

assert {
  res.status: eq 200
  res.body: isArray
  res.body[0].id: eq {{task_id_5}}
}

settings {
  encodeUrl: true
  timeout: 0
}
