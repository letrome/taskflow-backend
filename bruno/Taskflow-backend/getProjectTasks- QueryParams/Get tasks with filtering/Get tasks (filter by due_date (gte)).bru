meta {
  name: Get tasks (filter by due_date (gte))
  type: http
  seq: 5
}

get {
  url: {{baseurl}}/projects/{{project_id}}/tasks?due_date_from={{due_date_8}}
  body: none
  auth: bearer
}

params:query {
  due_date_from: {{due_date_8}}
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
}

assert {
  res.status: eq 200
  res.body: isArray
  res.body[0].id: eq {{task_id_8}}
  res.body[1].id: eq {{task_id_9}}
}

settings {
  encodeUrl: true
  timeout: 0
}
