meta {
  name: Login (with user A)
  type: http
  seq: 1
}

post {
  url: {{baseurl}}/auth/login
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{unique_email_user_a}}",
    "password": "{{unique_password_user_a}}"
  }
}

script:post-response {
  bru.setVar("token", res.body.token);
}

settings {
  encodeUrl: true
  timeout: 0
}
