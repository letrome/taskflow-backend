meta {
  name: Delete project
  type: http
  seq: 2
}

delete {
  url: {{baseurl}}/projects/{{invalid_project_id}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 404
  res.body.status: eq error
  res.body.message: eq Project not found
}

script:pre-request {
  bru.setVar("invalid_project_id", "696a61007c7baab47fdfc4f0")
}

settings {
  encodeUrl: true
  timeout: 0
}
