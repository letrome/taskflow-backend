meta {
  name: Delete task tag
  type: http
  seq: 4
}

delete {
  url: {{baseurl}}/tasks/{{task_id}}/tags/{{tag_id}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "tags": ["{{tag_id}}"]
  }
}

assert {
  res.status: eq 400
  res.body.status: eq error
  res.body.message: eq Tag does not exist
}

script:pre-request {
  bru.setVar("tag_id", "697d169c2f7f3a763f3b6b30");
}

settings {
  encodeUrl: true
  timeout: 0
}
