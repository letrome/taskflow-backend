meta {
  name: Login
  type: http
  seq: 1
}

post {
  url: {{baseurl}}/auth/login
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{unique_email}}",
    "password": "{{unique_password}}"
  }
}

assert {
  res.status: eq 200
  res.body.token: isString
  res.body.expires_in: eq 86400 seconds
}

settings {
  encodeUrl: true
  timeout: 0
}
